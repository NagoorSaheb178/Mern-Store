{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/static/(.*)",
            "dest": "/build/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/build/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/build/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.js"
        }
    ]
}
