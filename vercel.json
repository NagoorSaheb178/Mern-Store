{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/build/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/build/index.html"
        }
    ]
}
